type(vaccineclaim, class)
superclass(vaccineclaim, claim)
attribute(vaccineclaim, claim.reason_for_immunization)
attribute(vaccineclaim, claim.patient_is_immunocompromised)
attribute(vaccineclaim, claim.covid19_vaccine_brand)
attribute(vaccineclaim, claim.covid19_vaccination)

type(claim.reason_for_immunization, attributerelation)
domain(claim.reason_for_immunization, claim)
codomain(claim.reason_for_immunization, immunization_reason)
unique(claim.reason_for_immunization, yes)
total(claim.reason_for_immunization, no)


type(claim.patient_is_immunocompromised, attributerelation)
domain(claim.patient_is_immunocompromised, claim)
codomain(claim.patient_is_immunocompromised, boolean)
unique(claim.patient_is_immunocompromised, yes)
total(claim.patient_is_immunocompromised, no)

type(claim.covid19_vaccine_brand, attributerelation)
domain(claim.covid19_vaccine_brand, claim)
codomain(claim.covid19_vaccine_brand, vaccine_brand)
unique(claim.covid19_vaccine_brand, yes)
total(claim.covid19_vaccine_brand, no)

type(claim.covid19_vaccination, attributerelation)
domain(claim.covid19_vaccination, claim)
codomain(claim.covid19_vaccination, covid19_vaccination_event_obj)
unique(claim.covid19_vaccination, no)
total(claim.covid19_vaccination, no)

